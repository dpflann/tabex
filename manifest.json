{
  "name": "TabEx",
  "description": "Export tab data by posting it to an endpoint.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
      "default_title": "Export tab data",
      "default_icon": "favicon-256.png",
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": [
          "background.js"
      ],
      "persistent": false
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "options_ui": {
      "page": "options.html",
      "open_in_tab": false
  }
}
